{
  "name": "jquery-reference",
  "version": "0.0.1",
  "description": "프로젝트를 위한 연습",
  "scripts": {
    "dev": "node_modules/.bin/npm-run-all --parallel --continue-on-error pug sass serve",
    "serve": "node_modules/.bin/live-server --port=3001 --cors",
    "pug": "node_modules/.bin/pug src/ --pretty -w --out dist/",
    "sass": "node_modules/.bin/node-sass src/sass -w --source-map true -o dist/css"
  },
  "dependencies": {
    "jquery": "^3.2.1"
  },
  "devDependencies": {
    "live-server": "^1.2.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "pug-cli": "^1.0.0-alpha6"
  }
}